{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "Serilog.AspNetCore": "Warning"
      }
    },
    "File": {
      "Path": ".\\logs\\api.log"
    },
    "Logstash": {
      "disable": true
    }
  }
}
